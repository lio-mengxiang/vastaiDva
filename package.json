{
  "name": "@mx-design/cli-template",
  "version": "1.0.2",
  "description": "Cli Tools for mx Design Project",
  "keywords": [
    "mx-cli",
    "react cli",
    "webpack pack cli",
    "react library pack cli"
  ],
  "license": "MIT",
  "author": "1334196450@qq.com",
  "typings": "types/index.d.ts",
  "bin": {
    "mx": "./bin/index.js"
  },
  "files": [
    "lib",
    "bin",
    "types"
  ],
  "scripts": {
    "clean": "rimraf types lib coverage",
    "test": "jest --config ./jest.config.j$s",
    "coverage": "rimraf coverage && jest --config ./jest.config.js --coverage",
    "build:types": "rimraf types && tsc --outFile types/index.d.ts -d --emitDeclarationOnly",
    "build:es": "rimraf esm && mx buildLib --mode esm --less-2-css --copy-less",
    "build:cjs": "rimraf lib && mx buildLib --mode cjs --less-2-css --copy-less",
    "build:umd": "rimraf dist && mx buildLib --mode umd",
    "build": "yarn build:types && yarn build:cjs && yarn build:es && yarn build:umd",
    "release": "ts-node ./scripts/release.ts"
  },
  "dependencies": {
    "redux": "^4.1.2"
  },
  "devDependencies": {
    "@mx-design/release": "2.2.27",
    "@mx-design/cli": "1.0.16"
  },
  "browserslist": [
    "chrome 60",
    "Firefox 45",
    "safari 10"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
